namespace IR is
    use System;
    class CONTEXT is
        init() is si

        println(value: Object) is
            IO.Std.out.println("        " + value);
        si

        println(value: Object, comment: String) is
            IO.Std.out.println("        " + value);
        si
    si

    class CONTEXT_FACTORY is
        init() is si
        
        create_context(function: Semantic.Symbol.Function) -> CONTEXT is
            return new CONTEXT();
        si
    si
si

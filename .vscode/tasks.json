{
    "version": "0.1.0",
    "command": "ssh",
    "isBuildCommand": true,
    "args": ["lc", "/build/source/rewrite/build.sh"],
    "showOutput": "always",
    "problemMatcher": {
        "fileLocation": ["relative", "${workspaceRoot}"],
        "pattern": [{
            "regexp": "^(.*):\\s+(\\d+):\\s+(warn|error):\\s+(.*)$",
            "file": 1,
            "line": 2,
            "severity": 3,
            "message": 4
        }]
    }
}
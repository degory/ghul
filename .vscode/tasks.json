{
    "version": "0.1.0",
    "command": "ssh",
    // "isShellCommand": true,
    "isBuildCommand": true,
    "args": ["degory@dev.giantblob.com", "sudo", "llc-dev-build.sh"],
    // ssh degory@dev.giantblob.com sudo llc-dev-build.sh
    "showOutput": "always",
    "problemMatcher": {
        // "owner": "l",
        "fileLocation": ["relative", "${workspaceRoot}"],
        "pattern": [{
            "regexp": "^(.*):\\s+(\\d+):\\s+(warning|error):\\s+(.*)$",
            "file": 1,
            "line": 2,
            "severity": 3,
            "message": 4
        }]
    }
    // "runner": "terminal"
}
namespace Test.GenericInheritance is
    use System; 
    use Generic;

    class Base is

        test(i: int) -> int => i;
        test(s: short) -> short => s;
    si

    class OverrideInt: Base is
        test(i: int) -> int => i * 2;
    si

    class OverrideShort: Base is
        test(s: short) -> short => s * cast short(2);
    si

    class TestOverrideInt is
        test() is
            var t: OverrideInt;

            t.test(1234);
            t.test(cast short(1234));
        si
    si
si
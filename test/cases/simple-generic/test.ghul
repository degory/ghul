namespace Test.SimpleGenric is
    use Ghul;

    class Main is
        init() is
            let ic = new I[char](cast char(123));
            let ii = new I[int](456);

            let expect_int: int = ii.f(123);
            let expect_char: char = ic.f(cast char(123));

            let jc = new J[char](cast char(123));
            let ji = new J[int](456);

            ji.f(123);
            jc.f(cast char(123));

            ji.g(123);
            jc.g(cast char(123));
        si
    si

    class I[E] is
        b: E;

        init(b: E) is
            self.b = b;
        si

        f(x: E) -> E is
            return x;
        si
    si

    class J[F]: I[F] is
        c: F;
        i: I[F];
        j: J[F];
        ii: I[int];
        ji: J[int];

        g(a: F) -> F => a;

        init(c: F) is
            self.c = c;
            i = new I[F](c);
            j = self;
        si
    si
si
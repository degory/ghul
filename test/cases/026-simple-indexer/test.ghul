namespace Test.SimpleIndexer is
    use Ghul;

    class I is
        b: int;

        init(b: int) is
            self.b = b;
        si

        [i: int]: int public
            => b + i,
            = value is
                b = i - value;
            si
    si

    class Main is
        init() is
            var t = 0;
            let ii = new I(1000);

            t = t + ii[0];
            t = t + ii[1];
            t = t + ii[2];
            t = t + ii[3];
            t = t + ii[4];

            ii[0] = 50;
            ii[1] = 40;
            ii[2] = 30;
            ii[3] = 20;
            ii[4] = 10;
        si
    si
si
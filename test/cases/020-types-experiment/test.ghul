namespace Test.TypesExperiment is
    use Ghul;   

    trait I is
        f(i: byte, j: byte);
        f(i: int, j: int);
        f(i: int, j: ushort);
        g(i: int, j: int);
    si

    trait K is
        h(i: int, j: int);
    si

    trait J: I, K is
        f(i: int, j: int);
        g(i: word, j: word);
    si

    f(x: int, y: int) -> int
        => x + y;

    t(i: int) -> int => i / 2;

    class Main is
        a: int;
        b: int;
        c: int;

        t(i: int) -> int => i * 2;

        init() is
            var a: int;
            var b: int;
            
            var iii: I;
            var jjj: J; 
            var kkk: K;

            var i111 = 111;
            var j222 = 222;

            iii.f(111, 222);
            jjj.f(111, 222);
            jjj.g(111, 222);
            iii.f(i111, j222);
            jjj.f(i111, j222);
            jjj.f(b, 333);
            jjj.h(444, 555);
            kkk.h(666, 777);

            f(333, 444);

            t(t(2));

            f(t(1), t(t(2)));

            let q = f(t(i111), t(j222));

            let m = -q;

            t(q);

            a = 10;
            b = 20;
            c = a + b;
        si 
    si
si
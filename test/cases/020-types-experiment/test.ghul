namespace Test.TypesExperiment is
    use Ghul;   

    interface I is
        f(i: byte, j: byte);
        f(i: int, j: int);
        f(i: int, j: ushort);
        g(i: int, j: int);
    si

    interface J: I is
        f(i: int, j: int);
        g(i: word, j: word);
    si

    f(x: int, y: int)
        => x + y;

    class Main is
        init() is
            var iii: I;
            var jjj: J;

            var i111 = 111;
            var j222 = 222;

            iii.f(111, 222);
            jjj.f(111, 222);
            jjj.g(111, 222);
            iii.f(i111, j222);
            jjj.f(i111, j222);

            f(333, 444);
        si 
    si
si
namespace Test.TraitPropertyAssign is
    use System; 
    use Generic;

    class Main is
        init() is
            for i in 1::10 do

                IO.Std.err.println("test % is %" % [i, new Implement(i).test]: Object);
            od                    
        si        
    si
    
    trait Trait is
        test: int,
            = value;
    si

    class Implement: Object, Trait is
        _test: int;

        test: int => _test, = value is _test = value; si

        init(v: int) is
            test = v;
        si
    si    
si
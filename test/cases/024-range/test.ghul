namespace Test.Range is
    use Generic;

    class Main is
        test_exclusive(from: int, to: int) is
            IO.Std.err.println("range exclusive %..% => %" % [from, to, from..to]);

            for i in from..to do
                IO.Std.err.println("% of %" % [i, from..to]);
            od
        si

        test_inclusive(from: int, to: int) is
            IO.Std.err.println("range inclusive %::% => %" % [from, to, from::to]);

            for i in from::to do
                IO.Std.err.println("% of %" % [i, from::to]);
            od
        si

        init() is
            for from in -3::3 do
                for to in -3::3 do
                    test_exclusive(from, to);
                    test_inclusive(from, to);
                od
            od
        si 
    si
si
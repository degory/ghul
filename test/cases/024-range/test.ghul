namespace Test.Range is
    use Generic;

    class Main is
        test_exclusive(from: int, to: int) is
            IO.Std.err.println("%..% => %" % [from, to, from..to]);

            for i in from..to do
                IO.Std.err.println("% of %" % [i, from..to]);
            od
        si

        test_inclusive(from: int, to: int) is
            IO.Std.err.println("%::% => %" % [from, to, from::to]);

            for i in from::to do
                IO.Std.err.println("% of %" % [i, from::to]);
            od
        si

        init() is
            for from in -9..9 do
                for to in -9..9 do
                    test_exclusive(from, to);
                    test_inclusive(from, to);
                od
            od
        si 
    si
si
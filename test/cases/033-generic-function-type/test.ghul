namespace Test.Function is
    use Ghul;

    class Main is
        init() is
            var ti: Test[int];
            ti = new Test[int]();

            let ci = ti.get_closure();
            let ci_result = ci(123);

            var tc: Test[char];
            tc = new Test[char]();

            let cc = tc.get_closure();
            let cc_result = cc(cast char(123));
        si
    si

    class Test[Q] is
        init() is
        si

        get_closure() -> (Q) -> Q => (q: Q) -> Q => q;
    si
si

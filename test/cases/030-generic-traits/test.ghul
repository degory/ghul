namespace Test.GenericTraits is
    use Ghul;

    class Main is
        init() is
            let l = new List[int]();

            l[0] = 0;
            l[1] = 111;
            l[2] = 222;
            l[3] = 333;
            l[4] = l[0] + l[1] + l[2] + l[3];

            let i = l.elements;

            let hme = i.has_more_elements;
            let ne = i.next_element();

            let cl = l.copy();

            cl[5] = cl[4];
        si
    si

    trait Iterator[T] is
        has_more_elements: bool;
        next_element() -> T;
    si

    trait Iterable[T] is
        elements: Iterator[T];
    si
    
    trait List[T]: Iterable[T] is
        init() is            
        si

        copy() -> List[T];

        [index: int]: T = value;
    si
si
namespace Test is
    use Std = IO.Std;

    class Main is
        entry() static is
            // all valid:        
            let t: int = THING`[int,string].test_T(123);
            let u: string = THING`[int,string].test_U("hello");
            let tu: (int, string) = THING`[int,string].test_TU(123, "hello");
        si
    si

    class THING[T,U] is
        test_T(t: T) -> T static;

        test_U(u: U) -> U static;

        test_TU(t: T, u: U) -> (t: T, u: U) static;
    si    
si
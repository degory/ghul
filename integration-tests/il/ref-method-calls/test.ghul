namespace CallThroughStructReference is
    use IO.Std.write_line;

    entry(test: TEST ref) is
        @IL.output("il.out")
        test.set_value("Hello, World!");

        @IL.output("il.out")
        test.increment();

        @IL.output("il.out")
        test.write_value();

        @IL.output("il.out")
        test.value = "blah blah blah";
    si

    struct TEST is
        count: int public;
        value: string public;

        init(value: string) is
            self.count = 0;
            self.value = value;
        si

        set_value(value: string) is
            self.value = value;
        si

        increment() is
            self.count = self.count + 1;
        si

        write_value() is
            write_line(self.value);
        si
    si
si
namespace Test is
    @IF.not.v3()
    use Std = System.Console;
    @IF.not.v3()
    use object = System.Object;
    @IF.not.v3()
    use string = System.String;

    @IF.v3()
    use Std = IO.Std;

    class Main is
        test_int() is
            let x: int, y: int, z: int;

            @IL.output("il.out")
            x = y + z;

            @IL.output("il.out")
            x = y - z;

            @IL.output("il.out")
            x = y * z;

            @IL.output("il.out")
            x = y / z;
        si

        test_long() is
            let x: long, y: long, z: long;

            @IL.output("il.out")
            x = y + z;

            @IL.output("il.out")
            x = y - z;

            @IL.output("il.out")
            x = y * z;

            @IL.output("il.out")
            x = y / z;
        si

    si
si
use IO.Std.write_line;

entry() is
    let x = 10;
    let y = 20;

    write_line("interpolate {x+y} and {x-y} together");

    write_line("{x+x} at the beginning and {y-y} in the middle");

    write_line("now {x+y+x} in the middle and end with {x + x + y}");

    write_line("{x + 3}{x - y + 5}");
si


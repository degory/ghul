use IO.Std.write_line;

entry() is
    let x = 10;
    let y = 20;

    write_line("quoted not interpolated {{interpolate {{x+y}} and {{x-y}} together}}");

    write_line("quoted not interpolated {{x+x}} at the beginning and {{y-y}} in the middle}}");

    write_line("quoted not interpolated {{now {{x+y+x}} in the middle and end with {{x + x + y}}");

    write_line("quoted not interpolated {{x + 3}}{{x - y + 5}}");
si


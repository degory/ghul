namespace Test is
    use Std = IO.Std;

    class Main is
        entry() static is
            let t = new Thing();

            t.do_something(123);
            t.do_something("xyz");

            return;
        si
    si

    class GenericThing[T,U] is
        init() is            
        si

        do_something(t: T, u: U) -> Collections.List[T] is
            Std.out.write_line("do something t: " + t + " u: " + u);
        si        
    si

    class Thing: GenericThing[object,Thing] is
        init() is            
        si

        do_something(o: object) is
            do_something(o, self);
        si
    si    
si
namespace Test is
    @IF.not.v3()
    use Std = System.Console;
    @IF.not.v3()
    use object = System.Object;
    @IF.not.v3()
    use string = System.String;

    @IF.v3()
    use Std = IO.Std;

    class TEST[T,U] is
        // details of property definition too complex to test via IL snapshot - test would be too brittle - tested separately via integration test
        t: T;
        u: U;

        init(t: T, u: U) is
            @IL.output("il.out")
            self.t = t;
            @IL.output("il.out")
            self.u = u;
        si

        get_t() -> T is
            @IL.output("il.out")
            return t;
        si

        get_u() -> U is
            @IL.output("il.out")
            return u;
        si        
    si
si
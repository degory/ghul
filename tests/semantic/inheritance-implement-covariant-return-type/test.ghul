namespace Test is
    use System.String;

    class Main is
        entry() static is
            @IL.entrypoint()

            let a: Abstract = new CONCRETE();

            let b = a.copy();
        si
    si

    trait Abstract is
        copy() -> Abstract;
    si

    class CONCRETE: Abstract is
        init() is si

        // #460 Should error if return type in implemented method is not covariant
        copy() -> CONCRETE is
            System.Console.write_line("will copy CONCRETE");

            return cast CONCRETE(self.memberwise_clone());
        si
    si
si


namespace Test is
    class Main is
        init() static is
            @IL.entrypoint()

            let o: System.Object;
            let t: Thing;

            o = new Thing();
            t = new Thing();

            System.Console.write_line(o.to_string());
            System.Console.write_line(t.to_string());
        si
    si

    class Thing is
        init() is si

        @IL.name("ToString")
        to_string() -> System.String is
            return "A thing";            
        si
    si
si

@IL.stub()
namespace System is
    @IL.name("[mscorlib]System.Console")
    class Console is
        @IL.name("WriteLine")
        write_line(text: String) static;
    si
si


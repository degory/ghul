namespace Test is
    class Main is
        auto_static: int static;

        auto_instance: int;

        entry() static is
            @IL.entrypoint()

            auto_static = 123;

            let value = auto_static;

            System.Console.write_line("value is: " + value);

            let thing = new Thing(222);

            System.Console.write_line("thing is: " + thing.auto_instance);

            return;
        si
    si

    class Thing is
        auto_instance: int;

        init(value: int) is
            auto_instance = value * 2;
        si
    si

namespace Test is
    use System.Console;

    class Main is
        entry() static is
            @IL.entrypoint()

            let t = true;
            let f = false;

            if t then
                Console.write_line("expected: t is true");
            else
                Console.write_line("not expected: t is false");
            fi

            if f then
                Console.write_line("not expected: f is true");
            else
                Console.write_line("expected: f is false");
            fi
        si
    si
si
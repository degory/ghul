namespace Test is
    class Main is
        entry() static is
            @IL.entrypoint()

            let tds = new THING_DOER_STRUCT();
            tds.do_something(12345);

            let tdc = new THING_DOER_CLASS();
            tdc.do_something(33333);

            let ds: DoSomething;

            ds = tds.get_self();
            ds.do_something(54321);

            ds = tdc.get_self();
            ds.do_something(66666);
        si
    si

    trait DoSomething is
        do_something(value: int);
    si

    struct THING_DOER_STRUCT: DoSomething is
        init() is si

        do_something(value: int) is
            System.Console.write_line("struct do something: " + value);
        si

        get_self() -> DoSomething => self;        
    si

    class THING_DOER_CLASS: DoSomething is
        init() is si

        do_something(value: int) is
            System.Console.write_line("class do something: " + value);
        si

        get_self() -> DoSomething => self;        
    si
si
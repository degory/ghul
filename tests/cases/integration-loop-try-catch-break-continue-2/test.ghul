                    fi
                    
                    if i == 8 then
                        Console.write_line("outer: will break");
                        continue;                        
                    fi

                    Console.write_line("outer: did not continue or break");

                    try
                        for j in 0..10 do
                            Console.write_line("inner: " + j);
                                                        
                            if j >= 4 && j <= 5 then
                                Console.write_line("inner: will continue");
                                continue;
                            fi                                

                            if j >= 7 then
                                Console.write_line("inner: will break");
                                break;
                            fi

                            Console.write_line("inner: did not continue or break");
                        od

                    catch ex: Exception
                        Console.write_line("outer: caught: " + ex.message);
                    yrt        
                od

            catch ex: Exception
                Console.write_line("main: caught: " + ex.message);
            yrt
        si
    si

    class MyException: Exception is
        init(message: String) is super.init(message); si
    si   
si
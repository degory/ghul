    use System.String;
    use System.Exception;
    use System.Console;

    class Main is
        entry() static is
            @IL.entrypoint()

            try
                for i in 0..10 do
                    Console.write_line("outer: " + i);
    
                    if i == 3 || i == 7 then
                        Console.write_line("outer: will continue");

namespace Test is
    class Main is
        entry() static is
            @IL.entrypoint()

            let test = new TEST[int, System.String](12345, "hello");
            
            System.Console.write_line("external access t: " + test.t);
            System.Console.write_line("external access u: " + test.u);

            System.Console.write_line("self access t: " + test.get_t());
            System.Console.write_line("self access u: " + test.get_u());

            return;
        si
    si

    class TEST[T,U] is
        t: T;
        u: U;

        init(t: T, u: U) is
            self.t = t;
            self.u = u;
        si

        // check property access from within the class:
        get_t() -> T => t;
        get_u() -> U => u;
    si
si
namespace Test is
    use System;

    class Main is
        entry() static is
            @IL.entrypoint()

            let a = 10;
            let b = 20;
            let c = 30;

            if a <= b then
                Console.write_line("expected: 10 < 20");
            else
                Console.write_line("failed: 10 < 20");
            fi

            if a > b then
                Console.write_line("failed: 10 > 20");
            else
                Console.write_line("expected: 10 < 20");
            fi                

            if a <= b || c == 50 then
                Console.write_line("expected: 10 < 20 || 30 == 50");
            else
                Console.write_line("failed: 10 < 20 || 30 == 50");
            fi

            if a > b || c == 50 then
                Console.write_line("failed: 10 > 20 || 30 == 50");
            else
                Console.write_line("expected: 10 < 20 || 30 == 50");
            fi      
            
            if a <= b || c == 30 then
                Console.write_line("expected: 10 < 20 || 30 == 30");
            else
                Console.write_line("failed: 10 < 20 || 30 == 30");
            fi

            if a > b || c == 30 then
                Console.write_line("expected: 10 > 20 || 30 == 30");
            else
                Console.write_line("failed: 10 < 20 || 30 == 30");
            fi                            


            if a <= b || c != 50 then
                Console.write_line("expected: 10 < 20 || 30 != 50");
            else
                Console.write_line("failed: 10 < 20 || 30 != 50");
            fi

            if a > b || c != 50 then
                Console.write_line("expected: 10 > 20 || 30 != 50");
            else
                Console.write_line("failed: 10 < 20 || 30 != 50");
            fi      
            
            if a <= b || c != 30 then
                Console.write_line("expected: 10 < 20 || 30 != 30");
            else
                Console.write_line("failed: 10 < 20 || 30 != 30");
            fi

            if a > b || c != 30 then
                Console.write_line("failed: 10 > 20 || 30 != 30");
            else
                Console.write_line("expected: 10 < 20 || 30 != 30");
            fi    
            
            while c > 0 do
                Console.write_line("count down: " + c);

                c = c - 3;
            od

            Console.write_line("complete");            
        si
    si
si

@IL.stub()
namespace System is
    @IL.name("[mscorlib]System.Console")
    class Console is
        @IL.name("WriteLine")
        write_line(text: String) static;
    si
si
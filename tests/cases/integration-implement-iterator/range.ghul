namespace Ghul is
    use Collections;

    // trait Range[T]: Iterator[T] is
    //     from: T;
    //     to: T;
    // si

    struct INT_RANGE /* : Range[int] */ is
        from: int;
        to: int;

        current: int => from;

        init(from: int, to: int) is
            self.from = from;
            self.to = to;
        si

        move_next() -> bool is
            self.from = self.from + 1;
            return self.from < self.to;
        si        
    si

    struct INT_RANGE_INCLUSIVE /* : Range[int] */ is
        from: int;
        to: int;

        current: int => from;

        init(from: int, to: int) is
            self.from = from;
            self.to = to;
        si

        move_next() -> bool is
            self.from = self.from + 1;
            return self.from <= self.to;
        si        
    si
si

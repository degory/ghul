namespace Test is
    class Main is
        entry() static is
            @IL.entrypoint()

            let mt = new MultiThing();

            let dat: DoAThing = mt;
            let dse: DoSomethingElse = mt;

            System.Console.write_line("MultiThing.thing(111) = " + mt.thing(111));
            System.Console.write_line("DoAThing.thing(111) = " + dat.thing(111));
            System.Console.write_line("DoAThing.stuff(\"Hello\")...");
            dat.stuff("Hello");
            System.Console.write_line("DoSomethingElse.thing(111) = " + dse.thing(111));
            System.Console.write_line("DoSomethingElse.other(\"World\")...");
            dse.other("World");

            return;
        si
    si

    class MultiThing: DoAThing, DoSomethingElse is
        init() is
            super.init();
        si

        thing(i: int) -> int is
            System.Console.write_line("MultiThing.thing: " + i);
            return i * 2;
        si

        stuff(s: System.String) is
            System.Console.write_line("MultiThing.stuff: " + s);
        si
        
        other(s: System.String) is
            System.Console.write_line("MultiThing.other: " + s);
        si
    si

    trait DoAThing is
        thing(i: int) -> int;

        stuff(s: System.String);
    si

    trait DoSomethingElse is
        thing(i: int) -> int;

        other(s: System.String);
    si
si
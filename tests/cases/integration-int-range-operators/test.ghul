            od

            for i in 5 .. 10 do
                System.Console.write_line("expecting 5, 6, 7, 8, 9: " + i);
            od
                        
            for i in 5 :: 10 do
                System.Console.write_line("expecting 5, 6, 7, 8, 9, 10: " + i);
            od

            return;
        si

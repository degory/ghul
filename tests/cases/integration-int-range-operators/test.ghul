            for i in 0 .. -10 do
                System.Console.write_line("not expected: decending range: " + i);
            od

            for i in -5 .. 1 do
                System.Console.write_line("expecting -5, -4, -3, -2, -1, 0: " + i);
            od

            for i in -5 :: 0 do

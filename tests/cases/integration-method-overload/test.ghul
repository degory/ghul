namespace Test is
    use System.String;
    use System.Console;

    class Main is
        entry() static is
            @IL.entrypoint()

            let m = new Main(123);
            let n = new Main("hello");
            let o = new Main('x');

            m.test(123);
            m.test("hello");
            m.test('x');

            n.test(123);
            n.test("hello");
            n.test('x');

            o.test(123);
            o.test("hello");
            o.test('x');

            let tx = new X.Thing();
            let ty = new Y.Thing();

            m.test(123,tx);
            m.test("hello",tx);
            m.test('x', tx);

            m.test(123,ty);
            m.test("hello",ty);
            m.test('x', ty);
        si

        init(i: int) is
            Console.write_line("init int: " + i);
        si

        init(s: String) is
            Console.write_line("init String: " + s);            
        si
        
        init(c: char) is
            Console.write_line("init char: " + c);                        
        si

        test(i: int) is
            Console.write_line("method int: " + i);            
        si

        test(s: String) is
            Console.write_line("method String: " + s);            
        si

        test(c: char) is
            Console.write_line("method char: " + c);            
        si        

        test(i: int, t: X.Thing) is
            Console.write_line("method x int: " + i);
        si

        test(s: String, t: X.Thing) is
            Console.write_line("method x String: " + s);            
        si

        test(c: char, t: X.Thing) is
            Console.write_line("method x char: " + c);            
        si        

        test(i: int, t: Y.Thing) is
            Console.write_line("method y int: " + i);
        si

        test(s: String, t: Y.Thing) is
            Console.write_line("method y String: " + s);            
        si

        test(c: char, t: Y.Thing) is
            Console.write_line("method y char: " + c);            
        si        
    si
si

namespace X is
    class Thing is
        init() is si
    si
si

namespace Y is
    class Thing is
        init() is si
    si
si

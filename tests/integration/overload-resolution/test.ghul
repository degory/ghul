namespace Test is
    use System.Object;
    use System.String;
    use System.Console;

    class Main is
        entry() static is
            @IL.entrypoint()

            test("string");
            test(123);
            test(new Object(), "string");
            test("string", new Object());
            test("string", 123, 456);
            test(new Object(), 123, 456);
        si

        test(o: Object) static is
            Console.write_line("test(s: Object): " + o);            
        si
        
        test(s: String) static is
            Console.write_line("test(s: String): " + s);
        si

        test(i: int) static is
            Console.write_line("test(i: int): " + i);
        si
        
        test(o: Object, p: String) static is
            Console.write_line("test(o: Object, p: String): " + o + ", " + p);
        si

        test(s: String, t: Object) static is          
            Console.write_line("test(s: String, t: Object): " + s + ", " + t);
        si

        test(i: String, j: int, k: int) static is            
            Console.write_line("test(i: String, j: int, k: int): " + i + ", " + j + ", " + k);
        si    

        test(i: Object, j: int, k: int) static is
            Console.write_line("test(i: Object, j: int, k: int): " + i + ", " + j + ", " + k);
        si        
    si
si
namespace Test is
    @IF.not.v3()
    use Std = System.Console;
    @IF.not.v3()
    use object = System.Object;
    @IF.not.v3()
    use string = System.String;

    @IF.v3()
    use Std = IO.Std;

            
    class Main is
        entry() static is
            @IL.entrypoint()

            test("string");
            test(123);
            test(new object(), "string");
            test("string", new object());
            test("string", 123, 456);
            test(new object(), 123, 456);
        si

        test(o: object) static is
            Std.out.write_line("test(s: object): " + o);            
        si
        
        test(s: string) static is
            Std.out.write_line("test(s: string): " + s);
        si

        test(i: int) static is
            Std.out.write_line("test(i: int): " + i);
        si
        
        test(o: object, p: string) static is
            Std.out.write_line("test(o: object, p: string): " + o + ", " + p);
        si

        test(s: string, t: object) static is          
            Std.out.write_line("test(s: string, t: object): " + s + ", " + t);
        si

        test(i: string, j: int, k: int) static is            
            Std.out.write_line("test(i: string, j: int, k: int): " + i + ", " + j + ", " + k);
        si    

        test(i: object, j: int, k: int) static is
            Std.out.write_line("test(i: object, j: int, k: int): " + i + ", " + j + ", " + k);
        si        
    si
si
namespace Test is
    @IF.not.v3()
    use Std = System.Console;
    @IF.not.v3()
    use object = System.Object;
    @IF.not.v3()
    use string = System.String;

    @IF.v3()
    use Std = IO.Std;

    enum Blah is
        Bloop,
        Blip,
        Blergh
    si
    
    class Main is
        entry() static is
            @IL.entrypoint()

            let x = Blah.Bloop;
            let y = Blah.Blergh;

            if x == y then
                Std.out.write_line("not expected: " + x + " == " + y);
            else
                Std.out.write_line("expected: " + x + " != " + y);
            fi

            x = Blah.Blip;
            y = Blah.Bloop;

            if x != y then
                Std.out.write_line("expected: " + x + " != " + y);
            else
                Std.out.write_line("not expected: " + x + " == " + y);
            fi

            x = y;

            if x == y then
                Std.out.write_line("expected: " + x + " == " + y);
            else
                Std.out.write_line("not expected: " + x + " != " + y);
            fi
        si
    si
si
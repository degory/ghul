namespace Test is
    class Main is
        entry() static is
            @IL.entrypoint()

            let e = () -> System.String is
                System.Console.write_line("in e");                
                // no return;
            si;
            
            let f = (i: int) -> int is
                System.Console.write_line("in f: " + i);
                // no return;
            si;
            
            let g = () -> void is
                System.Console.write_line("in g");

                // no return
            si;

            let result_e = e();

            if result_e == null then
                System.Console.write_line("expected: e fallthrough returned null");
            else
                System.Console.write_line("not expected: e fallthrough returned non-null");
            fi

            let result_f = f(12345);

            if result_f == 0 then
                System.Console.write_line("expected: f fallthrough returned 0");
            else
                System.Console.write_line("not expected: f fallthrough returned " + result_f);
            fi
            
            g();

            System.Console.write_line("reached end");

            return;
        si
    si
si
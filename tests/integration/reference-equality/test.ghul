namespace Test is
    @IF.not.v3()
    use Std = System.Console;
    @IF.not.v3()
    use object = System.Object;
    @IF.not.v3()
    use string = System.String;

    @IF.v3()
    use Std = IO.Std;

    class Main is
        entry() static is
            @IL.entrypoint()

            let a = new A();
            let b = new B();
            let c = new C();

            let i: I = new X();
            let j: I = new Y();

            if a == b then // should warn comparison result is constant false
                Std.out.write_line("not expected: a == b");
            else
                Std.out.write_line("expected: a != b");                
            fi

            if a != b then // should warn comparison result is constant true
                Std.out.write_line("expected: a != b");
            else
                Std.out.write_line("not expected: a == b");                
            fi

            if b == c then
                Std.out.write_line("not expected: b == c");
            else
                Std.out.write_line("expected: b != c");                
            fi

            if b != c then
                Std.out.write_line("expected: b != c");
            else
                Std.out.write_line("not expected: b == c");                
            fi
            
            if i == j then
                Std.out.write_line("not expected: i == j");
            else
                Std.out.write_line("expected: i != j");                
            fi

            if i != j then
                Std.out.write_line("expected: i != j");
            else
                Std.out.write_line("not expected: i == j");                
            fi

            let aa = a;
            let bb = b;
            let cc: B = c;
            let ii = i;

            if aa == a then
                Std.out.write_line("expected: aa == a");
            else
                Std.out.write_line("not expected: aa != a");                
            fi

            if aa != a then
                Std.out.write_line("not expected: aa != a");
            else
                Std.out.write_line("expected: aa == a");                
            fi

            if bb == b then
                Std.out.write_line("expected: bb == b");
            else
                Std.out.write_line("not expected: bb != b");                
            fi

            if bb != b then
                Std.out.write_line("not expected: bb != b");
            else
                Std.out.write_line("expected: bb == b");                
            fi

            if cc == c then
                Std.out.write_line("expected: cc == c");
            else
                Std.out.write_line("not expected: cc  c");                
            fi

            if cc != c then
                Std.out.write_line("not expected: cc != c");
            else
                Std.out.write_line("expected: cc == c");                
            fi

            if ii == i then
                Std.out.write_line("expected: ii == i");
            else
                Std.out.write_line("not expected: ii != i");                
            fi

            if ii != i then
                Std.out.write_line("not expected: ii != i");
            else
                Std.out.write_line("expected: ii == i");
            fi
        si
    si

    class A is
        init() is si

        do_a() is
            Std.out.write_line("do A");
        si
    si

    class B is
        init() is si

        do_b() is
            Std.out.write_line("do B");
        si
    si
    
    class C: B is
        init() is si

        do_c() is
            Std.out.write_line("do C");
        si
    si    

    trait I is
        do_i() -> int;        
    si
    
    class X: I is
        init() is si

        do_i() -> int => 111;        
    si
    
    class Y: I is
        init() is si

        do_i() -> int => 222;
    si
si
namespace Test is
    class Main is
        entry() static is
            @IL.entrypoint()

            let t = new Thing(111);

            let u = new Thing(222);

            let z = t % u;

            System.Console.write_line("z is: " + z);
        si
    si

    class Thing is
        _value: int;

        init(value: int) is
            _value = value;
        si
        
        %(other: Thing) -> Thing is
            return new Thing(self._value + other._value);
        si

        to_string() -> System.String is
            return "T" + _value;
        si
    si
si


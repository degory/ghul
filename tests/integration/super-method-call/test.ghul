namespace Test is
    @IF.not.v3()
    use Std = System.Console;
    @IF.not.v3()
    use object = System.Object;
    @IF.not.v3()
    use string = System.String;

    @IF.v3()
    use Std = IO.Std;

    
    class Main is
        entry() static is
            @IL.entrypoint()

            let values = [
                new BASE(),
                new DERIVED(),
                new GENERIC[int]()
            ];

            for i in values do
                Std.out.write_line("call overridden() on " + i);

                i.overridden(12345);
            od            
        si
    si

    class BASE is
        init() is si

        overridden(value: int) is
            Std.out.write_line("BASE.overridden(" + value + ")");
        si
    si  

    class DERIVED: BASE is
        init() is si

        overridden(value: int) is
            Std.out.write_line("DERIVED.overridden(" + value + ")");

            super.overridden(value);
        si
    si

    class GENERIC[T]: DERIVED is
        init() is si

        overridden(value: int) is
            Std.out.write_line("GENERIC.overridden(" + value + ")");            

            super.overridden(value);
        si        
    si    
si
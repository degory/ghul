namespace Test is
    @IF.not.v3()
    use Std = System.Console;
    @IF.not.v3()
    use object = System.Object;
    @IF.not.v3()
    use string = System.String;

    @IF.v3()
    use Std = IO.Std;

    class Main is
        entry() static is
            @IL.entrypoint()

            Std.out.write_line("iterable"); 

            for i in new Iterable() do
                Std.out.write_line("index: " + i); 
            od

            Std.out.write_line("iterator"); 

            for i in new Iterable().iterator do
                Std.out.write_line("index: " + i); 
            od

            Std.out.write_line("iterator"); 

            for i in new Iterator() do
                Std.out.write_line("index: " + i); 
            od
        si
    si

    class Iterable is
        iterator: Iterator is return new Iterator(); si

        init() is            
        si        
    si

    class Iterator is
        _count: int;

        init() is
            _count = 5;
        si
        
        move_next() -> bool is
            _count = _count - 1;
            return _count >= 0;
        si

        current: int is
            return _count;    
        si                
    si
si


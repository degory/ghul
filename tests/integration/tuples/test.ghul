namespace Test is
    use System.String;
    use System.Console;

    class Main is
        entry() static is
            @IL.entrypoint()

            let x = (1, 2, 3);

            let y = ("hello", "world");

            Console.write_line("int tuple is: " + x);
            Console.write_line("string tuple is: " + y);

            Console.write_line("test int to string: " + test_int_to_string((123, 456, 789)));
        si

        test_int_to_string(int_tuple: (int, int,int)) -> (String, String) static =>
            (int_tuple._0.to_string(), int_tuple._2.to_string());
    si
si
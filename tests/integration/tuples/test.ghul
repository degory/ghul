namespace Test is
    @IF.not.v3()
    use Std = System.Console;
    @IF.not.v3()
    use object = System.Object;
    @IF.not.v3()
    use string = System.String;

    @IF.v3()
    use Std = IO.Std;

    class Main is
        entry() static is
            

            let x = (1, 2, 3);

            let y = ("hello", "world");

            Std.out.write_line("int tuple is: " + x);
            Std.out.write_line("string tuple is: " + y);

            Std.out.write_line("test int to string: " + test_int_to_string((123, 456, 789)));
        si

        test_int_to_string(int_tuple: (int, int, int)) -> (string, string) static =>
            (int_tuple._0.to_string(), int_tuple._2.to_string());
    si
si
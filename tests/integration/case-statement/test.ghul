namespace Test is
    class Main is
        entry() static is
            @IL.entrypoint()

            test_case(-50, "less than -1 or more than nine");
            test_case(-2, "less than -1 or more than nine");
            test_case(-1, "minus one");
            test_case(0, "zero");
            test_case(1, "one");
            test_case(5, "five");
            test_case(6, "more than five and less than ten");
            test_case(9, "more than five and less than ten");
            test_case(10, "less than -1 or more than nine");
            test_case(13, "unlucky");
            test_case(50, "less than -1 or more than nine");
        si

        test_case(value: int, expected: System.String) static is
            let result = run_case(value);

            if result =~ expected then
                System.Console.write_line("expected: " + value + " => " + result + " == " + expected);
            else
                System.Console.write_line("not expected: " + value + " => " + result + " != " + expected);
            fi            
        si

        run_case(value: int) -> System.String static is
            case value
            when -1:
                return "minus one";

            when 0:
                let result = "zero";
                return result;

            when 1:
                return "one";

            when 2:
                return "two";

            when 3:
                return "three";

            when 4:
                return "four";

            when 5:
                let result = "five";
                return result;

            when 6, 7, 8, 9:
                return "more than five and less than ten";

            when 13:
                return "unlucky";

            default
                return "less than -1 or more than nine";
            esac
            
            System.Console.write_line("should not get to here");
        si        
    si
si
use IO.Std.write_line;

entry() is
    let t = (a: string = "hello", b: int = 123);
    write_line("t is: " + t + " type: " + t.get_type());

    let u = (a: object = "hello", b: object = 123);
    write_line("u is: " + u + " type: " + u.get_type());

    let v = (a: object = ("hello", "world"), b: object = (123, 456));
    write_line("v is: " + v + " type: " + v.get_type());
si


namespace Test is
    @IF.not.v3()
    use Std = System.Console;
    @IF.not.v3()
    use object = System.Object;
    @IF.not.v3()
    use string = System.String;

    @IF.v3()
    use Std = IO.Std;

        
    use System.Threading.Monitor;
    use Collections.MAP;

    class Main is
        init() is
        si        

        entry() static is
            @IL.entrypoint()

            new Main().test();
        si
        
        test() is
            let map = new MAP[string,string]();

            map.add("A", "a");
            map.add("B", "b");
            map.add("C", "c");

            let result: string;

            for k in ["X", "A", "B", "C", "Y"] do
                let is_locked = false;
                try
                    Std.out.write_line("try enter monitor...");
                    Monitor.enter(self, is_locked ref);
                    Std.out.write_line("in monitor: locked is: " + is_locked);

                    if map.try_get_value(k, result ref) then
                        Std.out.write_line("got value '" + result + "' for key '" + k + "'");
                    else
                        Std.out.write_line("could not get value for key '" + k + "'");
                    fi   
                finally
                    Std.out.write_line("exit monitor...");
                    if is_locked then
                        Monitor.exit(self);                        
                    fi
                yrt
            od

            return;
        si        
    si
si
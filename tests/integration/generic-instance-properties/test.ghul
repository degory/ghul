namespace Test is
    @IF.not.v3()
    use Std = System.Console;
    @IF.not.v3()
    use object = System.Object;
    @IF.not.v3()
    use string = System.String;

    @IF.v3()
    use Std = IO.Std;

    class Main is
        entry() static is
            

            let test = new TEST[int, string](12345, "hello");
            
            Std.out.write_line("external access t: " + test.t);
            Std.out.write_line("external access u: " + test.u);

            Std.out.write_line("self access t: " + test.get_t());
            Std.out.write_line("self access u: " + test.get_u());

            return;
        si
    si

    class TEST[T,U] is
        t: T;
        u: U;

        init(t: T, u: U) is
            self.t = t;
            self.u = u;
        si

        // check property access from within the class:
        get_t() -> T => t;
        get_u() -> U => u;
    si
si
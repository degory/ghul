namespace Test is
    use System.Console;

    class Main is
        entry() static is
            @IL.entrypoint()

            Console.write_line("start");

            let i = 0;
            do
                Console.write_line("top outer, i is " + i);

                let j = 0;
                
                do
                    Console.write_line("top inner, i is " + i + ", j is " + j);
                    
                    j = j + 1;
                    Console.write_line("bottom inner, i is " + i + ", j is " + j);

                    if j > 2 then
                        Console.write_line("break inner");
                        break;
                    fi
                    
                od
                
                i = i + 1;

                Console.write_line("bottom outer, i is " + i);

                if i > 2 then
                    Console.write_line("break outer");
                    break;
                fi
            od
            
            Console.write_line("end");
        si
    si
si
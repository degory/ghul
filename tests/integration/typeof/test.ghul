namespace Test is
    @IF.not.v3()
    use Std = System.Console;
    @IF.not.v3()
    use object = System.Object;
    @IF.not.v3()
    use string = System.String;
    @IF.not.v3()
    use TYPE = System.Type2;

    @IF.v3()
    use Std = IO.Std;

    @IF.v3()
    use System.TYPE;
    
    use Collections.List;

    class Main is
        entry() static is
            @IL.entrypoint()

            Std.out.write_line("typeof int is " + typeof int);
            new TEST[int](123).test();

            Std.out.write_line("typeof string is " + typeof string);
            new TEST[string]("hello").test();

            Std.out.write_line("typeof (int,int,int) is " + typeof (int,int,int));
            new TEST[(int,int,int)]((111,222,333)).test();

            Std.out.write_line("typeof List[string] is " + typeof List[string]);
            new TEST[List[string]](["hello", "world"]);

            Std.out.write_line("typeof List is " + typeof List);
        si
    si

    class TEST[T] is
        value: T;
        init(value: T) is
            self.value = value;
        si

        type: TYPE => typeof T;

        test() is
            Std.out.write_line("value is: " + value + " type is: " + value.get_type() + " typeof is: " + typeof T); 
        si  
    si
si
namespace Test is
    use System.String;
    use System.Type2;
    use System.Console;

    use Collections.List;

    class Main is
        entry() static is
            @IL.entrypoint()

            Console.write_line("typeof int is " + typeof int);
            new TEST[int](123).test();

            Console.write_line("typeof String is " + typeof String);
            new TEST[String]("hello").test();

            Console.write_line("typeof (int,int,int) is " + typeof (int,int,int));
            new TEST[(int,int,int)]((111,222,333)).test();

            Console.write_line("typeof List[String] is " + typeof List[String]);
            new TEST[List[String]](["hello", "world"]);

            Console.write_line("typeof List is " + typeof List);
        si
    si

    class TEST[T] is
        value: T;
        init(value: T) is
            self.value = value;
        si

        type: Type2 => typeof T;

        test() is
            Console.write_line("value is: " + value + " type is: " + value.get_type() + " typeof is: " + typeof T); 
        si  
    si
si
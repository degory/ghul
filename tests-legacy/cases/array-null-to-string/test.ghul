namespace Test.ArrayNullToString is
    use System;

    use Generic;

    class Test[T] is
        init() is
        si

        test() is
            if T.isreference then
                System.Console.error.write_line(ClassName + ": element is a reference");
            else
                System.Console.error.write_line(ClassName + ": element is not a reference");
            fi
        si
    si

    class Main is
        init() is
            // IO.Std.err.FlushOnPrintln = true;

            let string_test = new Test[String]();
            string_test.test();

            let int_test = new Test[int]();
            int_test.test();

            let array_test = new Test[int[]]();
            array_test.test();

            let char_test = new Test[char]();
            char_test.test();

            let sa = ["a", "b", null, "c", "d"];
            let ia = [1, 2, 0, 0, 3, 4];

            System.Console.error.write_line("sa is: " + sa);
            System.Console.error.write_line("ia is: " + ia);
        si
    si
si

namespace Test.SimpleOperators is

    class A is
        value: int;

        init(v: int) is
            self.value = v;
        si
    si

    class Main is
        init() is
            var aa: A[];
            var aaa: A[][];
            
            aa = new A[](10);

            if aa.length == 10 then
                IO.Std.err.println("expected aa.length == 10");
            else
                IO.Std.err.println("not expected aa.length != 10");
            fi

            aaa = new A[][](20);
                      
            if aaa.length == 20 then
                IO.Std.err.println("expected aaa.length == 20");
            else
                IO.Std.err.println("not expected aaa.length != 20");
            fi

            IO.Std.err.println("aa is a " + aa.ClassName);
            IO.Std.err.println("aaa is a " + aaa.ClassName);
        si 
    si
si

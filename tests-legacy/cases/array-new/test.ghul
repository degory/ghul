namespace Test.SimpleOperators is

    class A is
        value: int;

        init(v: int) is
            self.value = v;
        si
    si

    class Main is
        init() is
            var aa: A[];
            var aaa: A[][];
            
            aa = new A[](10);

            if aa.length == 10 then
                System.Console.error.write_line("expected aa.length == 10");
            else
                System.Console.error.write_line("not expected aa.length != 10");
            fi

            aaa = new A[][](20);
                      
            if aaa.length == 20 then
                System.Console.error.write_line("expected aaa.length == 20");
            else
                System.Console.error.write_line("not expected aaa.length != 20");
            fi

            System.Console.error.write_line("aa is a " + aa.ClassName);
            System.Console.error.write_line("aaa is a " + aaa.ClassName);
        si 
    si
si

namespace Test.StaticModifier is
    class Main is
        init() is
            if 0 == 0 then
                System.Console.error.write_line("expect 0 == 0");
            else
                System.Console.error.write_line("do not expect 0 != 0");
            fi

            if 0 == 1 then
                System.Console.error.write_line("do not expect 0 == 1");
            else
                System.Console.error.write_line("expect 0 != 1");
            fi

            if 1 == 1 then
                System.Console.error.write_line("expect 1 == 1");
            elif 1 == 2 then
                System.Console.error.write_line("do not expect 1 == 2");
            elif 1 == 3 then
                System.Console.error.write_line("do not expect 1 == 3");
            else
                System.Console.error.write_line("do not expect");
            fi 
            
            if 0 == 1 then
                System.Console.error.write_line("do not expect 0 == 1");
            elif 0 == 2 then
                System.Console.error.write_line("do not expect 0 == 2");
            elif 0 == 3 then
                System.Console.error.write_line("do not expect 0 == 3");
            else
                System.Console.error.write_line("expect 0 != 1, 2, 3");
            fi

            if 3 == 1 then
                System.Console.error.write_line("do not expect 3 == 1");
            elif 3 == 2 then
                System.Console.error.write_line("do not expect 3 == 2");
            elif 3 == 3 then
                System.Console.error.write_line("expect 3 == 3");
            elif 3 == 4 then
                System.Console.error.write_line("do not expect 3 == 4");
            else
                System.Console.error.write_line("do not expect");
            fi            
        si
    si
si
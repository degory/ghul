namespace Tests.Fibonacci is
    use System;
    use Ghul;
    
    class Main is
        init() -> void is
            for i in 0::10 do
                IO.Std.err.println("fib(%) => %" % [i, fib(i)]: Object);
            od
        si

        fib(x: int) -> int is
            if x <= 1 then
                return x;
            else
                return fib(x - 2) + fib(x - 1);
            fi
        si
    si
si

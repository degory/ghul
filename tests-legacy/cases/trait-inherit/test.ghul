namespace Test40 is
    use System;
    
    trait Test is
        test();
    si

    trait ExtraTest: Test is
        extra_test();
    si

    class A: Object, Test is
        init() is si
        test() is System.Console.error.write_line("A.test()"); si
    si

    class B: Object, ExtraTest is
        init() is si
        test() is System.Console.error.write_line("B.test()"); si
        extra_test() is System.Console.error.write_line("B.extra_test()"); si
    si

    class Main is
        init() is
            var t: Test;
            var et: ExtraTest;

            t = new A();
            et = new B();

            t.test();

            et.test();
            et.extra_test();

            t = et;

            t.test();
        si
    si
si

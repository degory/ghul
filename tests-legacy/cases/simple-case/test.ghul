namespace Test.SimpleCase is
    class Main is
        test(v: int) is
            case v
            when 1:
                System.Console.error.write_line("v is 1");
            when 2, 3, 4:
                System.Console.error.write_line("v is 2, 3 or 4");
            when 5, 6:
                System.Console.error.write_line("v is 5 or 6");
            when 8:
                System.Console.error.write_line("v is 8");
            default
                System.Console.error.write_line("v is something else");
            esac
        si

        init() is
            for i in 0..10 do
                test(i);
            od
        si 
    si
si
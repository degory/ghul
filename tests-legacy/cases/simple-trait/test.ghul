namespace Test.SimpleInterface is
    trait I is
        f(i: int) -> int;
        g(g: int) -> int;
    si

    class C: System.Object, I is
        init() is si

        f(i: int) -> int => i * 2;
        g(i: int) -> int => i / 2;
    si

    class Main is
        init() is
            var i: I;

            i = new C();

            for n in 0..10 do
                System.Console.error.write_line("f(" + n + ") => " + i.f(n));
                System.Console.error.write_line("g(" + n + ") => " + i.g(n));
            od
        si 
    si
si
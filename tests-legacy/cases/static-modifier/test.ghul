namespace Test.StaticModifier is
    class Instance is
        init() is
        si

        instance_function() is
            System.Console.error.write_line("instance function");
        si

        static_function() static is
            System.Console.error.write_line("static function");
        si
    si

    class Main is
        init() is
            var instance = new Instance();

            instance.instance_function();
            instance.static_function();
            Instance.static_function();
        si
    si
si
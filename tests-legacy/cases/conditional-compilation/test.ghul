namespace Test is
    use System;

    @IF.dotnet()
    class Main is
        entry() static is
            @IL.entrypoint()

            let test = new Test("Hello world");

            test.test();
        si
    si
    
    @If.legacy()
    class Main is
        init() is
            let test = new Test(123456);

            test.test();
        si        
    si

    class Test is
        @IF.dotnet()
        message: String;

        @IF.legacy()
        count: int;

        @IF.dotnet()
        init(message: String) is
            self.message = message;
        si

        @IF.legacy()
        init(count: int) is
            self.count = count;           
        si
        
        test() is
            @IF.dotnet()
            Logging.DEBUG.write_line(message);

            @IF.legacy()
            IO.Std.err.println("count is: " + count);
        si
    si

    @IF.dotnet()
    class Broken is
        blah: DoesNotExist;

        test() is
            doSomethingDaft(bibble, bobble, blergh);
        si        
    si
    
si


namespace Test.SimpleIndexer is
    use Ghul;

    class I is
        b: int;

        init(b: int) is
            self.b = b;
        si

        [i: int]: int public
            => b + i,
            = value is
                b = i - value;
            si
    si

    class Main is
        init() is
            var t = 0;
            let ii = new I(1000);

            t = t + ii[0];
            t = t + ii[1];
            t = t + ii[2];
            t = t + ii[3];
            t = t + ii[4];

            ii[0] = 50;
            IO.Std.err.println("ii[0]: " + ii[0]);
            ii[1] = 40;
            IO.Std.err.println("ii[1]: " + ii[1]);
            ii[2] = 30;
            IO.Std.err.println("ii[2]: " + ii[2]);
            ii[3] = 20;
            IO.Std.err.println("ii[3]: " + ii[3]);
            ii[4] = 10;
            IO.Std.err.println("ii[4]: " + ii[4]);

            IO.Std.err.println("ii[0]: " + ii[0]);
            IO.Std.err.println("ii[1]: " + ii[1]);
            IO.Std.err.println("ii[2]: " + ii[2]);
            IO.Std.err.println("ii[3]: " + ii[3]);
            IO.Std.err.println("ii[4]: " + ii[4]);
        si
    si
si
    class EMPTY_SCOPE: Scope is
        _qualifier: string;
        name: string;
        qualified_name: string => name;
        symbols: Iterable[Symbol] => new LIST[Symbol]();
        type: Type => null;
        unspecialized_symbol: Symbol => null;
        is_trait: bool => false;

        init(name: string) is
            self.name = name;

            if name? && name.length > 0 then
                _qualifier = name + ".";                
            else
                _qualifier = "";
            fi
        si

        qualify(name: string) -> string => _qualifier + name;

        find_direct(name: string) -> Symbol => null;
        find_member(name: string) -> Symbol => null;
        find_enclosing(name: string) -> Symbol => null;

        find_direct_matches(prefix: string, results: MAP[string,Symbol]) is
        si

        find_member_matches(prefix: string, results: MAP[string,Symbol]) is
        si

        find_enclosing_matches(prefix: string, results: MAP[string,Symbol]) is
        si

        gen_dotted_name(buffer: System.Text.StringBuilder, want_trailing_dot: bool) is
            if want_trailing_dot then
                buffer.append(qualified_name);
            else
                buffer
                    .append(name)
                    .append(' ');
            fi
        si

        gen_type_spec(buffer: System.Text.StringBuilder) is
            throw new System.NotImplementedException("not implemented by " + get_type());
        si

        gen_class_name(buffer: System.Text.StringBuilder) is
            throw new System.NotImplementedException("not implemented by " + get_type());
        si

        gen_reference(buffer: System.Text.StringBuilder) is
            throw new System.NotImplementedException("not implemented by " + get_type());
        si

        gen_type(buffer: System.Text.StringBuilder) is

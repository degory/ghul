namespace Syntax.Tree.Body is
    use Source;

    class INNATE: NODE is
        name: Identifier.NODE;

        init(location: LOCATION, name: Identifier.NODE) is
            super.init(location);

            self.name = name;
        si

        accept(visitor: Visitor) is
            visitor.visit(self);
        si

        walk(visitor: Visitor) is
            if visitor.pre(self) then
                name.walk(visitor);
            fi

            accept(visitor);
        si
    si
si

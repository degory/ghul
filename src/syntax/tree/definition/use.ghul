namespace Syntax.Tree.Definition is
    
    use Source;

    class USE: NODE  is
        name: Identifier.NODE;
        use_: Identifier.NODE;
        
        init(location: LOCATION, name: Identifier.NODE, use_: Identifier.NODE) is
            super.init(location);

            self.name = name;
            self.use_ = use_;
        si

        accept(visitor: Visitor) is
            visitor.visit(self);
        si

        walk(visitor: Visitor) is
            if !visitor.pre(self) then
                if name? then
                    name.walk(visitor);
                fi

                if use_ then
                    use_.walk(visitor);
                fi                                
            fi
            
            accept(visitor);
        si
    si
si

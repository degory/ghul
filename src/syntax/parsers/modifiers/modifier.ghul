namespace Syntax.Parsers.Modifiers is
    class MODIFIER: Base[Trees.Modifiers.Modifier] is
        init() is
            super.init();

            add_parsers();
        si
        
        add_parsers() is
            add_parser(
                (context: CONTEXT) => new Trees.Modifiers.PUBLIC(context.location_and_next()),
                Lexical.TOKEN.PUBLIC
            );

            add_parser(
                (context: CONTEXT) => new Trees.Modifiers.PRIVATE(context.location_and_next()),
                Lexical.TOKEN.PRIVATE
            );

            add_parser(
                (context: CONTEXT) => new Trees.Modifiers.PROTECTED(context.location_and_next()),
                Lexical.TOKEN.PROTECTED
            );

            add_parser(
                (context: CONTEXT) => new Trees.Modifiers.STATIC(context.location_and_next()),
                Lexical.TOKEN.STATIC
            );

            add_parser(
                (context: CONTEXT) => new Trees.Modifiers.CONST(context.location_and_next()),
                Lexical.TOKEN.CONST
            );
        si

        other_token(context: CONTEXT) -> Trees.Modifiers.Modifier is
            return null;
        si
    si
si

namespace Syntax.Trees is
    use IO.Std;

    use System;    
    use Source;

    trait Node is
        location: LOCATION;

        id: int public;

        is_poisoned: bool;

        @IL.name("Equals")
        =~(other: Node) -> bool;

        >(other: Node) -> int;

        clone() -> Node;
        
        poison(should_poison: bool);

        poison();
    si
si

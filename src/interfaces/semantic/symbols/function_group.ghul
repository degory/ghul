namespace Semantic.Symbols is
    use Source;
    use Logging;

    use Types.Type;
    use IR.Values.Value;
    use IR.SymbolLoader;
    use IR.FunctionCaller;

    trait FunctionGroup: Symbol, Types.SettableTyped is
        count: int;
        is_empty: bool;

        functions: Collections.List[Function];

        specialize_function_group(type_map: Collections.Map[string,Type], owner: Generic) -> FunctionGroup;

        add(function: Function);

        add(fg: FunctionGroup);
    si
si

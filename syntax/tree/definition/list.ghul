namespace Syntax is
    namespace Tree is
        namespace Definition is
            use System;
            use Generic;
            use Source;
            class LIST : NODE , /* implements */ Iterable[NODE]  is
                definitions: List[NODE];
                init(location: LOCATION, definitions: Iterable[NODE]) -> void is
                    super.init(location);
                    self.definitions = new Vector[NODE](definitions);
                si

                Iterator: Iterator[NODE] is
                    return definitions.Iterator;
                si

                uses: Vector[Definition.USE] is
                    var result: Vector[Definition.USE] = new Vector[USE](definitions.Length);
                    for d in definitions do
                        if isa Definition.USE(d) then
                            result.add(cast Definition.USE(d));
                        fi
                    od
                    return result;
                si

                accept(visitor: Visitor) -> void is
                    visitor.visit(self);
                si

                walk(visitor: Visitor) -> void is
                    if !visitor.pre(self) then
                        for d in definitions do
                            d.walk(visitor);
                        od
                    fi
                    accept(visitor);
                si

            si

        si
    si
si

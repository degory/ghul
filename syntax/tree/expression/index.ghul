namespace Syntax is
    namespace Tree is
        namespace Expression is
            use Generic;
            use Source;
            class INDEX : NODE  is
                left: NODE;
                index: NODE;
                init(location: LOCATION, left: NODE, index: NODE) -> void is
                    super.init(location);
                    self.left = left;
                    self.index = index;
                si

                accept(visitor: Visitor) -> void is
                    visitor.visit(self);
                si

                walk(visitor: Visitor) -> void is
                    if !visitor.pre(self) then
                        left.walk(visitor);
                        index.walk(visitor);
                    fi
                    accept(visitor);
                si

                rewrite(m: Dict[Tree.NODE,Tree.NODE]) -> void is
                    left = cast NODE(m[left]);
                    index = cast NODE(m[index]);
                si

            si

        si
    si
si

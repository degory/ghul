namespace Syntax is
    namespace Tree is
        namespace Expression is
            use Generic;
            use Source;
            class MEMBER : NODE  is
                left: NODE;
                identifier: Identifier.NODE;
                init(location: LOCATION, left: NODE, identifier: Identifier.NODE) -> void is
                    super.init(location);
                    self.left = left;
                    self.identifier = identifier;
                si

                accept(visitor: Visitor) -> void is
                    visitor.visit(self);
                si

                walk(visitor: Visitor) -> void is
                    if !visitor.pre(self) then
                        left.walk(visitor);
                        identifier.walk(visitor);
                    fi
                    accept(visitor);
                si

                rewrite(m: Dict[Tree.NODE,Tree.NODE]) -> void is
                    left = cast NODE(m[left]);
                    identifier = cast Identifier.NODE(m[identifier]);
                si

            si

        si
    si
si

namespace Syntax is
    namespace Tree is
        namespace Type is
            use Generic;
            use Source;
            class STRUCTURED : NODE  is
                element: NODE;
                size: int is
                    return 8;
                si

                init(location: LOCATION, element: NODE) -> void is
                    super.init(location);
                    self.element = element;
                si

                accept(visitor: Visitor) -> void is
                    visitor.visit(self);
                si

                walk(visitor: Visitor) -> void is
                    if !visitor.pre(self) then
                        element.walk(visitor);
                    fi
                    accept(visitor);
                si

                rewrite(m: Dict[Tree.NODE,Tree.NODE]) -> void is
                    element = cast NODE(m[element]);
                si

            si

        si
    si
si

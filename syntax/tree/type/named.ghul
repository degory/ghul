namespace Syntax is
    namespace Tree is
        namespace Type is
            use Generic;
            use Source;
            class NAMED : NODE  is
                name: Identifier.NODE;
                init(location: LOCATION, name: Identifier.NODE) -> void is
                    super.init(location);
                    self.name = name;
                si

                size: int is
                    return 8;
                si

                accept(visitor: Visitor) -> void is
                    visitor.visit(self);
                si

                walk(visitor: Visitor) -> void is
                    if !visitor.pre(self) then
                        name.walk(visitor);
                    fi
                    accept(visitor);
                si

                rewrite(m: Dict[Tree.NODE,Tree.NODE]) -> void is
                    name = cast Identifier.NODE(m[name]);
                si

            si

        si
    si
si

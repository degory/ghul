namespace Syntax.Tree.Type is
    use Source;

    class POINTER: STRUCTURED  is
        init(location: LOCATION, element: NODE) is
            super.init(location, element);
        si

        accept(visitor: Visitor) is
            visitor.visit(self);
        si

        walk(visitor: Visitor) is
            if !visitor.pre(self) then
                element.walk(visitor);
            fi
            
            accept(visitor);
        si
    si
si

namespace Syntax is
    namespace Tree is
        namespace Statement is
            use Source;
            class DO : NODE  is
                condition: Expression.NODE;
                body: Statement.LIST;
                init(location: LOCATION, condition: Expression.NODE, body: Statement.LIST) -> void is
                    super.init(location);
                    self.condition = condition;
                    self.body = body;
                si

                accept(visitor: Visitor) -> void is
                    visitor.visit(self);
                si

                walk(visitor: Visitor) -> void is
                    if !visitor.pre(self) then
                        if condition != null then
                            condition.walk(visitor);
                        fi
                        body.walk(visitor);
                    fi
                    accept(visitor);
                si

            si

        si
    si
si
